<% if @memoryfeed_items.any? %>
<script type="text/javascript">
$(window).load(function() {
  var $container = $('#content');

  $container.imagesLoaded( function() {
    $container.isotope({
      itemSelector : '.newbox', 
   /* filter: '*',*/
    layoutMode: 'masonryHorizontal',
    animationEngine: 'jquery',
    animationOptions: {
       duration: 1500,
       easing: 'linear',
       queue: false,
     }


    });
  
  
    <%= render partial: 'shared/memoryfeed_item', collection: @memoryfeed_items %>
    });
});
</script>
<% end %>