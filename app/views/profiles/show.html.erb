<% provide(:title, @profile.first_name) %>
<div class="row">
  <aside class="span4">
    <section>
      <h1>
        <%= @profile.first_name + " " + @profile.last_name %></br>
        Born: <%= @profile.birthday %></br>
        Passed Away: <%= @profile.deathday %></br>
        Privacy: <%= @profile.privacy %></br>
      </h1>
    </section>
  </aside>
</div>
